{
  "name": "best-calc",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-win": "electron-builder --win"
  },
  "jest": {
    "verbose": true
  },
  "build": {
    "appId": "electron-calc",
    "mac": {
      "category": "yes"
    },
    "win": {
      "target": "NSIS"
   }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "jest": "^24.9.0"
  }
}
